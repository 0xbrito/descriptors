{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src"],
    "exclude": ["node_modules"],
    "includePattern": ".+\\.js(doc)?$"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs"
  }
}
